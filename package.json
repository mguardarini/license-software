{
  "name": "react-material-admin-template",
  "version": "0.0.1",
  "description": "Admin template using React and Material-UI",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html"
  },
  "author": "Mauro Guardarini",
  "license": "MIT",
  "dependencies": {
    "flexboxgrid": "6.3.1",
    "font-awesome": "4.7.0",
    "history": "4.7.2",
    "material-ui": "0.20.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router": "4.3.1",
    "react-tap-event-plugin": "3.0.3",
    "recharts": "1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "9.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.24.5",
    "chai": "4.1.2",
    "chalk": "2.4.1",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "eslint": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "eslint-watch": "4.0.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "isparta": "4.1.0",
    "mocha": "5.2.0",
    "node-sass": "4.9.2",
    "npm-run-all": "4.1.3",
    "open": "0.0.5",
    "postcss-loader": "2.1.6",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "2.1.0",
    "replace": "1.0.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "sinon": "6.1.3",
    "sinon-chai": "3.2.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.16.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [
    "react",
    "reactjs",
    "material",
    "material-ui",
    "template",
    "admin"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
